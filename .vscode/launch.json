{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch app.mjs",
      "program": "${workspaceFolder}/backend/app.mjs",
      "runtimeArgs": ["--experimental-modules"],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
